Cell["Maximum Likelihood Estimation", "Text",
 CellChangeTimes->{{3.673292679496593*^9, 3.673292697096819*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"loglikelihood", "[", 
    RowBox[{"mu_", ",", "sigma_"}], "]"}], ":=", 
   RowBox[{"Plus", "@@", 
    RowBox[{"Map", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Log", "[", 
        RowBox[{"PDF", "[", 
         RowBox[{
          RowBox[{"NormalDistribution", "[", 
           RowBox[{"mu", ",", "sigma"}], "]"}], ",", "#"}], "]"}], "]"}], 
       "&"}], ",", 
      RowBox[{"z", "[", 
       RowBox[{"[", 
        RowBox[{"5001", ";;", "10000"}], "]"}], "]"}]}], "]"}]}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"\[Mu]", ",", "\[Sigma]"}], "}"}], "=", 
   RowBox[{"FindArgMax", "[", 
    RowBox[{
     RowBox[{"loglikelihood", "[", 
      RowBox[{"x", ",", "y"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"x", ",", "0.1"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"y", ",", "0.9"}], "}"}]}], "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{"range", "=", 
