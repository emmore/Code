  3.6732723082796392`*^9, 3.673272308479628*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"RandomWalkMetropolis", "[", 
   RowBox[{"pdf_", ",", "start_", ",", "delta_", ",", "times_"}], "]"}], ":=",
   "\n", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "f", "}"}], ",", "\n", "\t", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "[", "current_", "]"}], ":=", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"w", ",", "new"}], "}"}], ",", "\n", "\t\t", 
        RowBox[{
         RowBox[{"new", "=", 
          RowBox[{"current", "+", 
           RowBox[{"RandomReal", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "delta"}], ",", "delta"}], "}"}], "]"}]}]}], ";", 
         "\n", "\t\t", 
         RowBox[{"w", "=", 
          RowBox[{"Min", "[", 
           RowBox[{"1", ",", 
            RowBox[{
             RowBox[{"pdf", "[", "new", "]"}], "/", 
             RowBox[{"pdf", "[", "current", "]"}]}]}], "]"}]}], ";", "\n", 
         "\t\t", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"RandomReal", "[", "]"}], "<", "w"}], ",", "new", ",", 
           "current"}], "]"}]}]}], "]"}]}], ";", "\n", "\t", 
     RowBox[{"Return", "[", 
      RowBox[{"NestList", "[", 
       RowBox[{"f", ",", "start", ",", "times"}], "]"}], "]"}], ";"}]}], "\n",
    "]"}]}]], "Code",
 CellChangeTimes->{{3.673271608122766*^9, 3.673271610885076*^9}, {
  3.673273492152546*^9, 3.6732735908195057`*^9}, {3.673273683665358*^9, 
